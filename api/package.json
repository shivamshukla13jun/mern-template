{
  "name": "ecom-api",
  "version": "1.0.0",
  "description": "E-commerce Microservices API with Redis and RabbitMQ",
  "main": "src/server.ts",
  "scripts": {
    "dev": "nodemon ",
    "build": "tsc && tsc-alias && npm run copy:assets",
    "copy:assets": "cpx \"src/**/*.ejs\" dist/",
    "start": "npm run build && pm2 start dist/server.js",
    "start:dev": "npm run build && node dist/server.js",
    "stop": "pm2 start dist/server.js",
    "start:pm2": "npm run build && pm2 start dist/server.js && sudo env PATH=$PATH:/usr/bin pm2 startup systemd -u root --hp /root && pm2 save",
    "restart:pm2": "npm run build && pm2 restart dist/server.js",
    "log:pm2": "pm2 logs",
    "type-check:watch": "tsc --noEmit --watch",
    "exit:pm2": "pm2 kill"
  },
  "dependencies": {
    "accesscontrol": "^2.2.1",
    "agenda": "^5.0.0",
    "amqplib": "^0.10.9",
    "axios": "^1.8.1",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.53.0",
    "compression": "^1.8.0",
    "connect-mongo": "^5.1.0",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.1",
    "file-type": "^21.0.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.6.1",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.1",
    "mime-types": "^3.0.1",
    "mongoose": "^7.5.0",
    "mongoose-autopopulate": "^1.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.3",
    "puppeteer": "^24.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "redis": "^5.8.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "swagger2-to-postmanv2": "^2.1.0",
    "uuid": "^11.1.0",
    "validator": "^13.12.0",
    "winston": "^3.17.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/axios": "^0.9.36",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.13",
    "@types/crypto-js": "^4.2.2",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.17",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-session": "^1.18.1",
    "@types/file-type": "^10.6.0",
    "@types/jest": "^29.5.4",
    "@types/json2csv": "^5.0.7",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mime-types": "^3.0.1",
    "@types/morgan": "^1.9.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.19.17",
