    "@types/nodemailer": "^6.4.17",
    "@types/puppeteer": "^5.4.7",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.12.2",
    "cpx": "^1.2.1",
    "nodemon": "^3.0.1",
    "pm2": "^5.4.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.11",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
